{
  "name": "eslint-plugin-react-query",
  "version": "1.0.0",
  "description": "ESLint plugin for React Query",
  "keywords": [
    "react-query",
    "eslint",
    "plugin"
  ],
  "author": "Eliya Cohen",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup --minify",
    "dev": "tsup --watch --dts --sourcemap",
    "test": "vitest src/**/*.test.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "publish": "pnpm build && pnpm publish"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@typescript-eslint/types": "^5.40.1",
    "@typescript-eslint/utils": "^5.40.1",
    "eslint": "^8.26.0",
    "tsup": "^6.3.0",
    "typescript": "4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  }
}
